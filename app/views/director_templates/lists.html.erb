<h1> List of directors </h1>


<table border='1'>
  <tr>
  <th>ID</th>
  <th>Image</th>
  <th>Name</th>
  <th>dob</th>
  <th></th>
  </tr>

  <% Director.all.each do |a_director| %>

<tr>
  <td><%= a_director.id %></td>
  <td><img src="<%= a_director.image%>" alt="headshot" width="100"></td>
  <td><%= a_director.name%></td>
  <td><%= a_director.dob%></td>
  <td><a href="/directions/<%= a_director.id%>">show details</a></td>
</tr>
<% end %>
</table>
